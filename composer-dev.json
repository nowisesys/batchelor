{
    "name": "bmc/batchelor",
    "version": "2.2.1",
    "description": "Batch job queue manager (web interface and services)",
    "type": "project",
    "keywords": ["batchelor", "queue", "task"],
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "Anders Lövgren",
            "email": "anders.lovgren@bmc.uu.se"
        },
        {
            "name": "Anders Lövgren",
            "email": "andlov@nowise.se"
        }
    ],
    "repositories": [
        {
